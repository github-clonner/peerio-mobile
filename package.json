    "url-regex": "4.1.1"
  },
  "devDependencies": {
    "appium": "1.9.2-beta.2",
    "authorize-ios": "1.0.4",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.3",
    "babel-plugin-transform-async-to-bluebird": "1.1.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-exponentiation-operator": "6.24.1",
