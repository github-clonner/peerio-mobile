    "readline": "1.3.0",
    "repeating-cli": "2.0.0",
    "sinon": "1.17.7",
    "sinon-chai": "2.14.0",
    "socket.io": "2.1.1",
    "standard-version": "4.0.0",
    "typescript": "2.0.10",
    "user-home-cli": "1.0.1",
    "validate-commit-message": "3.0.1",
    "webdriverio": "4.9.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "helpMessage": "Convention: https://goo.gl/3TgTsh"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:lint && npm run test:prettier"
    }
  }
}
